<?xml version="1.0" encoding="UTF-8"?>

<component name="ca.firstvoices.lifecycle.service" version="1.0.0">
  <extension point="lifecycle" target="org.nuxeo.ecm.core.lifecycle.LifeCycleService">
    <lifecycle defaultInitial="draft" name="fv-alt1-lifecycle">
      <states>
        <state description="Draft" initial="true" name="draft">
          <transitions>
            <transition>to_needs_review</transition>
          </transitions>
        </state>
        <state description="Needs Review" name="needs_review">
          <transitions>
            <transition>to_incomplete</transition>
            <transition>to_draft</transition>
            <transition>to_complete</transition>
          </transitions>
        </state>
        <state description="Incomplete" name="incomplete">
          <transitions>
            <transition>to_needs_review</transition>
            <transition>to_complete</transition>
          </transitions>
        </state>
        <state description="Complete" name="complete">
          <transitions/>
        </state>
      </states>
      <transitions>
        <transition destinationState="needs_review" name="to_needs_review">
          <description></description>
        </transition>
        <transition destinationState="draft" name="to_draft">
          <description></description>
        </transition>
        <transition destinationState="incomplete" name="to_incomplete">
          <description></description>
        </transition>
        <transition destinationState="complete" name="to_complete">
          <description></description>
        </transition>
        <transition destinationState="needs_review" name="to_needs_review">
          <description></description>
        </transition>
        <transition destinationState="complete" name="to_complete">
          <description></description>
        </transition>
      </transitions>
    </lifecycle>
    <lifecycle defaultInitial="draft" name="fv-alt2-lifecycle">
      <states>
        <state description="Draft" initial="true" name="draft">
          <transitions>
            <transition>to_needs_review</transition>
          </transitions>
        </state>
        <state description="Needs Review" name="needs_review">
          <transitions>
            <transition>to_draft</transition>
            <transition>to_needs_editing</transition>
            <transition>to_complete</transition>
          </transitions>
        </state>
        <state description="needs_editing" name="needs_editing">
          <transitions>
            <transition>to_needs_review</transition>
            <transition>to_complete</transition>
          </transitions>
        </state>
        <state description="Complete" name="complete">
          <transitions>
            <transition>to_needs_editing</transition>
          </transitions>
        </state>
      </states>
      <transitions>
        <transition destinationState="needs_review" name="to_needs_review">
          <description></description>
        </transition>
        <transition destinationState="draft" name="to_draft">
          <description></description>
        </transition>
        <transition destinationState="needs_editing" name="to_needs_editing">
          <description></description>
        </transition>
        <transition destinationState="complete" name="to_complete">
          <description></description>
        </transition>
        <transition destinationState="needs_review" name="to_needs_review">
          <description></description>
        </transition>
        <transition destinationState="complete" name="to_complete">
          <description></description>
        </transition>
        <transition destinationState="needs_editing" name="to_needs_editing">
          <description></description>
        </transition>
      </transitions>
    </lifecycle>
    <lifecycle defaultInitial="new" name="fv-crm-lifecycle">
      <states>
        <state description="New" initial="true" name="new">
          <transitions>
            <transition>to_contact_info_confirmed</transition>
          </transitions>
        </state>
        <state description="Contract Info Confirmed" name="contact_info_confirmed">
          <transitions>
            <transition>to_active_user</transition>
            <transition>to_needs_follow_up</transition>
            <transition>to_inactive_user</transition>
          </transitions>
        </state>
        <state description="Active User" name="active_user">
          <transitions>
            <transition>to_needs_follow_up</transition>
            <transition>to_inactive_user</transition>
          </transitions>
        </state>
        <state description="Inactive User" name="inactive_user">
          <transitions>
            <transition>to_active_user</transition>
            <transition>to_needs_follow_up</transition>
          </transitions>
        </state>
        <state description="Needs Follow Up" name="needs_follow_up">
          <transitions>
            <transition>to_needs_contact_info</transition>
            <transition>to_active_user</transition>
            <transition>to_inactive_user</transition>
          </transitions>
        </state>
        <state description="Needs Contact Info" name="needs_contact_info">
          <transitions>
            <transition>to_active_user</transition>
            <transition>to_inactive_user</transition>
          </transitions>
        </state>
      </states>
      <transitions>
        <transition destinationState="contact_info_confirmed" name="to_contact_info_confirmed">
          <description></description>
        </transition>
        <transition destinationState="active_user" name="to_active_user">
          <description></description>
        </transition>
        <transition destinationState="inactive_user" name="to_inactive_user">
          <description></description>
        </transition>
        <transition destinationState="needs_follow_up" name="to_needs_follow_up">
          <description></description>
        </transition>
        <transition destinationState="needs_follow_up" name="to_needs_follow_up">
          <description></description>
        </transition>
        <transition destinationState="inactive_user" name="to_inactive_user">
          <description></description>
        </transition>
        <transition destinationState="needs_follow_up" name="to_needs_follow_up">
          <description></description>
        </transition>
        <transition destinationState="active_user" name="to_active_user">
          <description></description>
        </transition>
        <transition destinationState="active_user" name="to_active_user">
          <description></description>
        </transition>
        <transition destinationState="inactive_user" name="to_inactive_user">
          <description></description>
        </transition>
        <transition destinationState="needs_contact_info" name="to_needs_contact_info">
          <description></description>
        </transition>
        <transition destinationState="active_user" name="to_active_user">
          <description></description>
        </transition>
        <transition destinationState="inactive_user" name="to_inactive_user">
          <description></description>
        </transition>
      </transitions>
    </lifecycle>
    <lifecycle defaultInitial="todo" name="fv-kanban-lifecycle">
      <states>
        <state description="To Do" initial="true" name="todo">
          <transitions>
            <transition>to_in_progress</transition>
          </transitions>
        </state>
        <state description="In Progress" name="in_progress">
          <transitions>
            <transition>to_done</transition>
            <transition>to_Todo</transition>
          </transitions>
        </state>
        <state description="Done" name="done">
          <transitions>
            <transition>to_in_progress</transition>
          </transitions>
        </state>
      </states>
      <transitions>
        <transition destinationState="in_progress" name="to_in_progress">
          <description></description>
        </transition>
        <transition destinationState="done" name="to_done">
          <description></description>
        </transition>
        <transition destinationState="todo" name="to_Todo">
          <description></description>
        </transition>
        <transition destinationState="in_progress" name="to_in_progress">
          <description></description>
        </transition>
      </transitions>
    </lifecycle>
    <lifecycle defaultInitial="waiting_for_admin" name="fv-request-lifecycle">
      <states>
        <state description="Waiting for Admin" initial="true" name="waiting_for_admin">
          <transitions>
            <transition>to_review_in_progress</transition>
            <transition>to_escalated_to_fpcc</transition>
            <transition>to_accepted</transition>
            <transition>to_rejected</transition>
          </transitions>
        </state>
        <state description="Review in Progress" name="review_in_progress">
          <transitions>
            <transition>to_missing_info</transition>
            <transition>to_accepted</transition>
            <transition>to_escalated_to_fpcc</transition>
            <transition>to_waiting_for_admin</transition>
            <transition>to_rejected</transition>
          </transitions>
        </state>
        <state description="Accepted" name="accepted">
          <transitions>
            <transition>to_review_in_progress</transition>
            <transition>to_rejected</transition>
          </transitions>
        </state>
        <state description="Rejected" name="rejected">
          <transitions>
            <transition>to_review_in_progress</transition>
            <transition>to_accepted</transition>
          </transitions>
        </state>
        <state description="Missing Information" name="missing_info">
          <transitions>
            <transition>to_accepted</transition>
            <transition>to_rejected</transition>
          </transitions>
        </state>
        <state description="Escalated to FPCC" name="escalated_to_fpcc">
          <transitions>
            <transition>to_missing_info</transition>
            <transition>to_accepted</transition>
            <transition>to_waiting_for_admin</transition>
            <transition>to_rejected</transition>
          </transitions>
        </state>
      </states>
      <transitions>
        <transition destinationState="review_in_progress" name="to_review_in_progress">
          <description></description>
        </transition>
        <transition destinationState="escalated_to_fpcc" name="to_escalated_to_fpcc">
          <description></description>
        </transition>
        <transition destinationState="rejected" name="to_rejected">
          <description></description>
        </transition>
        <transition destinationState="accepted" name="to_accepted">
          <description></description>
        </transition>
        <transition destinationState="waiting_for_admin" name="to_waiting_for_admin">
          <description></description>
        </transition>
        <transition destinationState="rejected" name="to_rejected">
          <description></description>
        </transition>
        <transition destinationState="accepted" name="to_accepted">
          <description></description>
        </transition>
        <transition destinationState="missing_info" name="to_missing_info">
          <description></description>
        </transition>
        <transition destinationState="escalated_to_fpcc" name="to_escalated_to_fpcc">
          <description></description>
        </transition>
        <transition destinationState="rejected" name="to_rejected">
          <description></description>
        </transition>
        <transition destinationState="review_in_progress" name="to_review_in_progress">
          <description></description>
        </transition>
        <transition destinationState="accepted" name="to_accepted">
          <description></description>
        </transition>
        <transition destinationState="review_in_progress" name="to_review_in_progress">
          <description></description>
        </transition>
        <transition destinationState="rejected" name="to_rejected">
          <description></description>
        </transition>
        <transition destinationState="accepted" name="to_accepted">
          <description></description>
        </transition>
        <transition destinationState="waiting_for_admin" name="to_waiting_for_admin">
          <description></description>
        </transition>
        <transition destinationState="missing_info" name="to_missing_info">
          <description></description>
        </transition>
        <transition destinationState="rejected" name="to_rejected">
          <description></description>
        </transition>
        <transition destinationState="accepted" name="to_accepted">
          <description></description>
        </transition>
      </transitions>
    </lifecycle>
    <lifecycle defaultInitial="New" name="fv-lifecycle">
      <states>
        <state description="A new document is created." initial="true" name="New">
          <transitions>
            <transition>Enable</transition>
            <transition>Disable</transition>
            <transition>Publish</transition>
          </transitions>
        </state>
        <state description="Document is enabled and visible to dialect members." name="Enabled">
          <transitions>
            <transition>RevertToNew</transition>
            <transition>Disable</transition>
            <transition>Publish</transition>
          </transitions>
        </state>
        <state description="Document is only available to language administrators or recorders."
          name="Disabled">
          <transitions>
            <transition>Enable</transition>
          </transitions>
        </state>
        <state description="" name="Published">
          <transitions>
            <transition>Unpublish</transition>
            <transition>Republish</transition>
            <transition>RevertToNew</transition>
            <transition>Disable</transition>
          </transitions>
        </state>
        <state description="Temp state for legacy life cycle" name="project">
          <transitions>
            <transition>Publish</transition>
          </transitions>
        </state>
        <state
          description="Intermediate state before publishing again (a state can't do back to itself)."
          name="Republish">
          <transitions>
            <transition>Publish</transition>
          </transitions>
        </state>
      </states>
      <transitions>
        <transition destinationState="Enabled" name="Enable">
          <description>Document has been approved.</description>
        </transition>
        <transition destinationState="Disabled" name="Disable">
          <description></description>
        </transition>
        <transition destinationState="Published" name="Publish">
          <description></description>
        </transition>
        <transition destinationState="Disabled" name="Disable">
          <description>Document has been disabled.</description>
        </transition>
        <transition destinationState="Published" name="Publish">
          <description></description>
        </transition>
        <transition destinationState="New" name="RevertToNew">
          <description></description>
        </transition>
        <transition destinationState="Enabled" name="Enable">
          <description>Make document available to members again.</description>
        </transition>
        <transition destinationState="Enabled" name="Unpublish">
          <description></description>
        </transition>
        <transition destinationState="Disabled" name="Disable">
          <description></description>
        </transition>
        <transition destinationState="Republish" name="Republish">
          <description></description>
        </transition>
        <transition destinationState="New" name="RevertToNew">
          <description></description>
        </transition>
        <transition destinationState="Published" name="Publish">
          <description></description>
        </transition>
        <transition destinationState="Published" name="Publish">
          <description></description>
        </transition>
      </transitions>
    </lifecycle>
  </extension>
</component>

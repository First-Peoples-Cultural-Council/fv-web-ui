<?xml version="1.0"?>
<component name="ca.firstvoices.workers.fvdataworkmanagers">
  <extension point="queues" target="org.nuxeo.ecm.core.work.service">
    <queue id="addConfusablesToAlphabet">
      <category>addConfusablesToAlphabet</category>
      <clearCompletedAfterSeconds>600</clearCompletedAfterSeconds>
      <maxThreads>1</maxThreads>
      <name>Queue for adding confusables to archive's alphabet</name>
    </queue>

    <queue id="cleanConfusablesForWordsAndPhrases">
      <category>cleanConfusablesForWordsAndPhrases</category>
      <clearCompletedAfterSeconds>600</clearCompletedAfterSeconds>
      <maxThreads>1</maxThreads>
      <name>Queue for cleaning confusables for words and phrases</name>
    </queue>
  </extension>
</component>